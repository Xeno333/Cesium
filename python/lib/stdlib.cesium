c{
    #include<stdio.h>
}c

cref func fopen(i8* fn, i8* op) : i64
cref func fclose(u64 f) : i32
cref func fread(u8* buffer, u64 size, u64 count, u64 stream) : i64

cref func printf(i8* str, ...) : i32


func fget(u8* fn, mut i8* block, u64 size) : u8* {
    u64 f = (fopen(fn, "rb") : u64)

    fread(block, 1, size, f)

    fclose(f)

    return block
}

func fsize(u8* fb) : i64